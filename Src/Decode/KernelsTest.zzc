						{{{.f=0xA0}}}, {{{.f=0xA1}}}, {{{.f=0xB2}}}, {{{.f=0xC3}}},   {{{.f=0xA4}}}, {{{.f=0xA5}}}, {{{.f=0xB6}}}, {{{.f=0xC7}}},   {{{.f=0xA8}}}, {{{.f=0xA9}}}, {{{.f=0xBA}}}, {{{.f=0xCB}}},   {{{.f=0xAC}}}, {{{.f=0xA0}}}, {{{.f=0xB1}}}, {{{.f=0xC2}}},   
