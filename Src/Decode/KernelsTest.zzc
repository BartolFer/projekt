						{{{.f=0xA6}}}, {{{.f=0xA7}}}, {{{.f=0xB8}}}, {{{.f=0xC9}}},   {{{.f=0xAA}}}, {{{.f=0xAB}}}, {{{.f=0xBC}}}, {{{.f=0xC0}}},   {{{.f=0xA1}}}, {{{.f=0xA2}}}, {{{.f=0xB3}}}, {{{.f=0xC4}}},   {{{.f=0xA5}}}, {{{.f=0xA6}}}, {{{.f=0xB7}}}, {{{.f=0xC8}}},   
