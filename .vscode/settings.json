	"emeraldwalk.runonsave": {
		"commands": [
			{
				"autoShowOutputPanel": "error",
				"showElapsed": false,
				"cmd": "make ide",
				"match": "\\.zzc$",
			},
			{
				"autoShowOutputPanel": "error",
				"showElapsed": false,
				"cmd": "make ide",
				"match": "\\.cl$",
			},
		]
	},
